{
  "kind": "collectionType",
  "collectionName": "translation_posts",
  "info": {
    "singularName": "translation-post",
    "pluralName": "translation-posts",
    "displayName": "Translation Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "post": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::hashtag-post.hashtag-post",
      "inversedBy": "translations"
    },
    "translator": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "translation_posts"
    },
    "translation": {
      "type": "text",
      "required": false
    },
    "role": {
      "type": "enumeration",
      "enum": [
        "en_tr",
        "en_nl",
        "nl_tr",
        "nl_en",
        "tr_en",
        "tr_nl"
      ],
      "required": true
    },
    "has_translated": {
      "type": "boolean",
      "default": false
    }
  }
}
