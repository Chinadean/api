{
  "kind": "collectionType",
  "collectionName": "hashtag_posts",
  "info": {
    "singularName": "hashtag-post",
    "pluralName": "hashtag-posts",
    "displayName": "Hashtag Post"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "text": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true
    },
    "image": {
      "allowedTypes": ["images", "videos"],
      "type": "media",
      "multiple": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "twitter_image": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "text",
      "maxLength": 50
    },
    "hashtag": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::hashtag.hashtag",
      "inversedBy": "posts"
    },
    "translation": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::translation-post.translation-post",
      "mappedBy": "post"
    }
  }
}
