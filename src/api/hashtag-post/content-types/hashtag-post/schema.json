{
  "kind": "collectionType",
  "collectionName": "hashtag_posts",
  "info": {
    "singularName": "hashtag-post",
    "pluralName": "hashtag-posts",
    "displayName": "Hashtag Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "text": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "text",
      "maxLength": 50,
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images", "videos"],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "twitter_image": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "hashtag": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::hashtag.hashtag",
      "inversedBy": "posts"
    },
    "creator": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "created_posts"
    },
    "translator": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::translator.translator",
      "inversedBy": "posts"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "hashtag_posts"
    }
  }
}
